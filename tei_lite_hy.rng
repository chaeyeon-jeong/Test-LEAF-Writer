               <oneOrMore>
                  <ref name="hy_model.graphicLike"/>
                  <zeroOrMore>
                     <ref name="hy_model.global"/>
                  </zeroOrMore>
               </oneOrMore>
            </choice>
            <zeroOrMore>
               <ref name="hy_model.divBottom"/>
               <zeroOrMore>
                  <ref name="hy_model.global"/>
